<?xml version="1.0" encoding="utf-8" ?>
<plugin id="cordova-plugin-hello-world" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>HelloWorld</name>
    <js-module name="HelloWorld" src="www/HelloWorld.js">
        <clobbers target="HelloWorld" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="HelloWorld">
                <param name="ios-package" value="HelloWorld" />
            </feature>
        </config-file>
        <source-file src="src/ios/HelloWorld.m" />
    </platform>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="HelloWorld">
                <param name="android-package" value="cordova.plugin.helloworld.HelloWorld" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/HelloWorld.java" target-dir="src/cordova/plugin/helloworld/HelloWorld" />
    </platform>
</plugin>